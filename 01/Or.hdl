 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Nand(a=a, b=a, out=aNot);
    Nand(a=b, b=b, out=bNot);
    Nand(a=aNot, b=bNot, out=out);
}
